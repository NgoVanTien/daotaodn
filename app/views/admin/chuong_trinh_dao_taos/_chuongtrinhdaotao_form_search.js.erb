$(document).ready(function(){
  $(".btn-filter").on("click", function(){
    if($(".filtertable").css("display") == "none"){
      $(".filtertable").slideDown();
    }
    else {
      $(".filtertable").slideUp();
    }
  });

  $(".search-alert").on("click", function(){
    swal({
    title: "Đang Tìm Kiếm",
    timer: 2000,
    showConfirmButton: false,
    type: "success"
    });
  });
});
